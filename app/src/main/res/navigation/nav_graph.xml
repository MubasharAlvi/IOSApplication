<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenClass">

    <fragment
        android:id="@+id/splashScreenClass"
        android:name="com.example.ioscameraandroidapp.SplashScreenClass"
        android:label="splash_screen"
        tools:layout="@layout/splash_screen" >
        <action
            android:id="@+id/action_splashScreenClass_to_cameraHomeScreenClass"
            app:destination="@id/cameraHomeScreenClass" />
    </fragment>
    <fragment
        android:id="@+id/cameraHomeScreenClass"
        android:name="com.example.ioscameraandroidapp.CameraHomeScreenClass"
        android:label="camera_home_screen"
        tools:layout="@layout/camera_home_screen" >
        <action
            android:id="@+id/action_cameraHomeScreenClass_to_beautyModeClass"
            app:destination="@id/beautyModeClass" />
    </fragment>
    <fragment
        android:id="@+id/beautyModeClass"
        android:name="com.example.ioscameraandroidapp.BeautyModeClass"
        android:label="beauty_mode"
        tools:layout="@layout/beauty_mode" />
</navigation>